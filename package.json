    "mockery": "^2.0.0",
    "sinon": "^2.3.4",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": ">=4.x"
  },
  "homepage": "https://github.com/ilyakam/gulp-pug-linter#readme",
